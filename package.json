{
  "name": "compote-fp",
  "version": "0.1.0",
  "license": "MIT",
  "main": "compote-fp.js",
  "types": "compote-fp.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest",
    "jest": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "20.0.5",
    "jest": "20.0.4",
    "ts-jest": "20.0.7",
    "typescript": "2.4.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.d.ts",
      "!node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.test)\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
